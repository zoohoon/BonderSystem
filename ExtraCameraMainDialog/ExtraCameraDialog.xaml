<Window x:Class="ExtraCameraMainDialog.ExtraCameraDialog"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ExtraCameraMainDialog"
             xmlns:view="clr-namespace:UcExtraCameraDialogView;assembly=UcExtraCameraDialogView"
             xmlns:vm="clr-namespace:ExtraCameraDialogVM;assembly=ExtraCameraDialogVM"
             mc:Ignorable="d" 
             WindowStyle="SingleBorderWindow"
             Background="#333333"
             Closing="Window_Closing"
             d:DesignHeight="960" d:DesignWidth="960">
    <Window.DataContext>
        <DataTemplate DataType="{x:Type vm:ExtraCameraDialogViewModel}">
            <view:ExtraCameraDialogView/>
        </DataTemplate>
    </Window.DataContext>
    
    <Grid>
        <view:ExtraCameraDialogView/>
    </Grid>
</Window>
